

@{
    ViewBag.Title = "Success";
}

<h2>Success</h2>
@using (Html.BeginForm("InsertarExcel", "T_Perfil", FormMethod.Get))
{
    <div class="md-form m-2 text-center">
        <button type="submit" class="btn btn-md btn-success">AGREGAR</button>
    </div>

}


@using (Html.BeginForm("Salir", "T_Perfil", FormMethod.Get))
{
    <div class="md-form m-2 text-center">
        <button type="submit" class="btn btn-md btn-success">REGRESAR</button>
    </div>
}
<div class="row ">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>DESCRIPCION</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var olista in (List<Web.Marcacion.Models.PerfilExcel>)ViewBag.ListPerfil)
                {
                    <tr>
                        <td>@olista.Descripcion</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

