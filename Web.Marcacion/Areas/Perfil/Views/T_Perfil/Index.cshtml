@model List<Web.Marcacion.Models.T_Perfil>
@{
    ViewBag.Title = "Index";
}


<h2>Perfil</h2>
@using (Html.BeginForm("Create", "T_Perfil", FormMethod.Get))
{
    <button type="submit" class="btn btn-primary">NUEVO</button>
}

@using (Html.BeginForm("Importar", "T_Perfil", FormMethod.Get))
{
    <button type="submit" class="btn btn-primary">IMPORT</button>
}

@using (Html.BeginForm("Delete", "T_Perfil", FormMethod.Get))
{
    <div class="row ">
        <div class="col-lg-12">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>IDPERFIL</th>
                        <th>DESCRIPCION</th>
                        <th>ESTADO</th>
                        <th>ACCIONES</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var olista in Model)
                    {
                        <tr>
                            <td>@olista.ID_Perfil</td>
                            <td>@olista.Descripcion</td>
                            <td>@olista.Estado</td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = olista.ID_Perfil })  |
                                @Html.ActionLink("Detail", "Detail", new { id = olista.ID_Perfil }) |
                                @Html.ActionLink("Delete", "Delete", new { id = olista.ID_Perfil })

                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

}